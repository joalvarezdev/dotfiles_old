alias py=$HOME/personal/projects/python

### GIT ###
alias gbh='git branch | head -20'
alias gslh='git stash list | head -20'
alias gsp='git stash pop'

alias d3=~/personal/projects
alias conf=~/personal/config/configuration
alias hours='code ~/personal/registros'

# INFOSIS
source $configuration_path/.infosis 
