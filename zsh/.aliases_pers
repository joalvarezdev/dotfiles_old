configuration_path=~/personal/config/configuration/zsh

alias soz="source ~/.zshrc" 

# Siempre iniciar con TMUX
tmux &> /dev/null

source $configuration_path/.drive

### SHORCUTS ### 

# SSH SHORCUTS
source $configuration_path/.ssh_shorcuts

alias op="xdg-open ."
